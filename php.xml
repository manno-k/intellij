<application>
  <component name="MessDetector">
    <phpmd_settings_migrated />
    <phpmd_settings>
      <MessDetectorConfiguration tool_path="/usr/local/bin/phpmd" />
    </phpmd_settings>
  </component>
  <component name="PhpCodeSniffer">
    <phpcs_settings_migrated />
    <phpcs_settings>
      <PhpCSConfiguration standards="MySource;PEAR;PSR1;PSR2;Squiz;WordPress;WordPress-Core;WordPress-Docs;WordPress-Extra;WordPress-VIP;Zend" tool_path="/usr/local/bin/phpcs" />
    </phpcs_settings>
  </component>
</application>